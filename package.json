{
  "name": "gitpanda",
  "displayName": "gitpanda",
  "description": "A VSCode extension for improved worflow with git branches",
  "version": "0.0.1",
  "icon": "public/images/gitpanda.png",
  "publisher": "MargaritaBusygina",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitpanda.copy",
        "title": "Copy Branch Name"
      },
      {
        "command": "gitpanda.deleteCurrentBranch",
        "title": "Delete Current Branch"
      },
      {
        "command": "gitpanda.deleteMergedBranches",
        "title": "Delete Merged Branches"
      },
      {
        "command": "gitpanda.shipAll",
        "title": "Ship All Changes"
      },
      {
        "command": "gitpanda.branchInfo",
        "title": "Show Branch Info"
      },
      {
        "command": "gitpanda.statusBarHandler",
        "title": "Show Branch Tools"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
